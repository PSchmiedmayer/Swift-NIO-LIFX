{
    "name": "Swift",
    "dockerFile": "Dockerfile",
    "runArgs": [
        "--cap-add=SYS_PTRACE",
        "--security-opt", "seccomp=unconfined"
    ],
    "appPort": [
        "56700:56700/udp"
    ],
    "extensions": [
        "pvasek.sourcekit-lsp--dev-unofficial",
        "vadimcn.vscode-lldb"
    ],
    "settings": {
        "lldb.adapterType": "bundled",
        "lldb.executable": "/usr/bin/lldb",
        "terminal.integrated.shell.linux": "/bin/bash"
    }
}
